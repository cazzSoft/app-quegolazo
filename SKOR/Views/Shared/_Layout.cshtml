                <input type="password" name="txtClaveRegistro" id="txtClaveRegistro" value="" class="form_input required" placeholder="Crea una clave para ¡Qué Golazo!" />

                <label><input type="checkbox" id="chkSeePasswordRegistro" /> Ver clave</label>
                <input type="button" name="btnRegistrar" class="form_submit" id="btnRegistrar" value="Crear Cuenta" />

                @*<h5>- Redes sociales? -</h5>
                    <div class="signup_social">
                        <a href="http://www.facebook.com/" class="signup_facebook external">FACEBOOK</a>
                        <a href="http://www.twitter.com/" class="signup_twitter external">TWITTER</a>
                    </div>*@
            </div>
            <div class="close_popup_button">
                <a href="#" class="close-popup"><img src="~/images/icons/black/menu_close.png" alt="" title="" /></a>
            </div>
        </div>
    </div>

    <!-- Forgot Password Popup -->
    <div class="popup popup-forgot">
        <div class="content-block">
            <h4>¿OLVIDO SU CLAVE?</h4>
